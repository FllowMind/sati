package com.jyu.sati.entity;

import java.util.Date;

public class User {

	// 七种用户类型
	public final static int USER_TYPE_PERSON = 1;// 个人用户
	public final static int USER_TYPE_COMPANY = 2;// 企业用户
	public final static int USER_TYPE_AGENCY = 3;// 中介机构单位
	public final static int USER_TYPE_COLLEGE = 4;// 高校单位用户
	public final static int USER_TYPE_SCIENTIFIC = 5;// 科研单位用户
	public final static int USER_TYPE_PLATFORM = 6;// 平台管理员
	public final static int USER_TYPE_SUPER = 7;// 超级管理员

	// 用户状态
	public final static int USER_STATUS_LIMIT = -1;// 限制使用状态
	public final static int USER_STATUS_NORMAL = 1;// 正常使用状态

	private String userId;// 用户账号
	private String userName;// 用户呢称
	private String password;// 密码
	private Integer userType;// 用户类型
	private Integer personId;// 个人用户ID
	private Integer companyId;// 企业用户ID
	private Integer universityId;// 高校用户ID
	private Integer sruId;// 科研单位用户ID
	private Integer agencyId;// 中介用户ID
	private Integer administratorId;// 平台管理员ID
	private Integer superAdministratorId;// 超级管理员ID
	private Date registerTime;// 注册时间
	private Date preloginTime;// 上次登录时间
	private Date currentloginTime;// 当前登录时间
	private Date preupdateTime;// 上次更新时间
	private Integer auditId;// 审核信息ID
	private Integer status;// 用户状态
	private Integer contactInfoId;//联系人信息id

	public String getUserId() {
		return userId;
	}

	public void setUserId(String userId) {
		this.userId = userId;
	}

	public String getUserName() {
		return userName;
	}

	public void setUserName(String userName) {
		this.userName = userName;
	}

	public String getPassword() {
		return password;
	}

	public void setPassword(String password) {
		this.password = password;
	}

	public Integer getUserType() {
		return userType;
	}

	public void setUserType(Integer userType) {
		this.userType = userType;
	}

	public Integer getPersonId() {
		return personId;
	}

	public void setPersonId(Integer personId) {
		this.personId = personId;
	}

	public Integer getCompanyId() {
		return companyId;
	}

	public void setCompanyId(Integer companyId) {
		this.companyId = companyId;
	}

	public Integer getUniversityId() {
		return universityId;
	}

	public void setUniversityId(Integer universityId) {
		this.universityId = universityId;
	}

	public Integer getSruId() {
		return sruId;
	}

	public void setSruId(Integer sruId) {
		this.sruId = sruId;
	}

	public Integer getAgencyId() {
		return agencyId;
	}

	public void setAgencyId(Integer agencyId) {
		this.agencyId = agencyId;
	}

	public Integer getAdministratorId() {
		return administratorId;
	}

	public void setAdministratorId(Integer administratorId) {
		this.administratorId = administratorId;
	}

	public Integer getSuperAdministratorId() {
		return superAdministratorId;
	}

	public void setSuperAdministratorId(Integer superAdministratorId) {
		this.superAdministratorId = superAdministratorId;
	}

	public Date getRegisterTime() {
		return registerTime;
	}

	public void setRegisterTime(Date registerTime) {
		this.registerTime = registerTime;
	}

	public Date getPreloginTime() {
		return preloginTime;
	}

	public void setPreloginTime(Date preloginTime) {
		this.preloginTime = preloginTime;
	}

	public Date getCurrentloginTime() {
		return currentloginTime;
	}

	public void setCurrentloginTime(Date currentloginTime) {
		this.currentloginTime = currentloginTime;
	}

	public Date getPreupdateTime() {
		return preupdateTime;
	}

	public void setPreupdateTime(Date preupdateTime) {
		this.preupdateTime = preupdateTime;
	}

	public Integer getAuditId() {
		return auditId;
	}

	public void setAuditId(Integer auditId) {
		this.auditId = auditId;
	}

	public Integer getStatus() {
		return status;
	}

	public void setStatus(Integer status) {
		this.status = status;
	}


	public Integer getContactInfoId() {
		return contactInfoId;
	}

	public void setContactInfoId(Integer contactInfoId) {
		this.contactInfoId = contactInfoId;
	}

	@Override
	public String toString() {
		return "User [userId=" + userId + ", userName=" + userName + ", password=" + password + ", userType=" + userType
				+ ", personId=" + personId + ", companyId=" + companyId + ", universityId=" + universityId + ", sruId="
				+ sruId + ", agencyId=" + agencyId + ", administratorId=" + administratorId + ", superAdministratorId="
				+ superAdministratorId + ", registerTime=" + registerTime + ", preloginTime=" + preloginTime
				+ ", currentloginTime=" + currentloginTime + ", preupdateTime=" + preupdateTime + ", auditId=" + auditId
				+ ", status=" + status + ", contactInfoId=" + contactInfoId + "]";
	}

}