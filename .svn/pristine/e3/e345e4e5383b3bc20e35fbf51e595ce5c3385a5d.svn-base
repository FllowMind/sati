package com.jyu.sati.entity;

import java.util.Date;

public class Role {

	public final static int ROLE_SUPER_ADMIN = 1;// 超级管理员
	public final static int ROLE_PLATFORM = 2;// 平台管理员
	public final static int PERSON = 3;// 个人用户
	public final static int COMPANY = 4;// 企业用户
	public final static int AGENCY = 5;// 中介机构用户
	public final static int COLLEAGE = 6;// 高校用户
	public final static int SCIENTIFIC = 7;// 科研单位用户

	public final static String ROLE_NAME_SUPER_ADMIN = "super_admin";// 超级管理员
	public final static String ROLE_NAME_PLATFORM = "platform_admin";// 平台管理员
	public final static String ROLE_NAME_PERSON = "person";// 个人用户
	public final static String ROLE_NAME_COMPANY = "company";// 企业用户
	public final static String ROLE_NAME_AGENCY = "agency";// 中介机构用户
	public final static String ROLE_NAME_COLLEAGE = "colleage";// 高校用户
	public final static String ROLE_NAME_SCIENTIFIC = "scientific";// 科研单位用户

	private Integer roleId;

	private String roleName;

	private String description;

	private Date createTime;

	private Date operateTime;

	private Integer status;

	public Integer getRoleId() {
		return roleId;
	}

	public void setRoleId(Integer roleId) {
		this.roleId = roleId;
	}

	public String getRoleName() {
		return roleName;
	}

	public void setRoleName(String roleName) {
		this.roleName = roleName;
	}

	public String getDescription() {
		return description;
	}

	public void setDescription(String description) {
		this.description = description;
	}

	public Date getCreateTime() {
		return createTime;
	}

	public void setCreateTime(Date createTime) {
		this.createTime = createTime;
	}

	public Date getOperateTime() {
		return operateTime;
	}

	public void setOperateTime(Date operateTime) {
		this.operateTime = operateTime;
	}

	public Integer getStatus() {
		return status;
	}

	public void setStatus(Integer status) {
		this.status = status;
	}

	@Override
	public String toString() {
		return "Role [roleId=" + roleId + ", roleName=" + roleName + ", description=" + description + ", createTime="
				+ createTime + ", operateTime=" + operateTime + ", status=" + status + "]";
	}

}