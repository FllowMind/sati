}

.info-title span{
    padding-right: 2px;
    color: #999;
    display: inline-block;
    font-size: 13px;
}
.item-time{
    display: inline-block;
    height: 20px;
    width: 20px;
    margin-left: 2px;
    margin-right: 6px;
    margin-bottom: -4px;
    background: url("../images/icons.jpg") no-repeat;
    background-position: -304px -54px;
}

.supply-require .list .info-content{
    line-height: 22px;
    color: #999;
    font-size: 13px;
    white-space: nowrap;
    text-overflow: ellipsis;
