}
.supply-require .list .info-title a{
    color: #244496;
    font-weight: bold;
    margin-bottom: -10px;
    text-decoration: none;
    display: inline-block;
    white-space: nowrap;
    word-break: keep-all;
    overflow: hidden;
    text-overflow: ellipsis;
