body{
    margin: 0;
    padding: 0;
}

/*默认样式*/
ol,ul,li{
    list-style: none;
}

a{
    text-decoration: none;
    display: block;
}

img{
    border: none;
    display: block;
}

/*去除默认样式*/
textarea,select,input{
    -webkit-appearance: none;
    -moz-appearance: none;
    -o-appearance: none;
    appearance: none;
}

.clearfloat{
    zoom: 1;
}

.clearfloat:after {
    display: block;
    clear: both;
    content: "";
    visibility: hidden;
    height: 0;
}


.public-container{
    margin: 0 auto;
    width: 984px;
}

/*头部*/
.index-header-title{
    height: 36px;
    line-height: 36px;
}
 /*����960px*/
@media screen and (min-width:960px){
    .index-header-title {
       background: url("../images/top_bg_pc.png") no-repeat;
    }
}
/*С��100px,��Ҫ�����ֻ���*/
@media screen and (max-device-width: 1000px){
    .index-header-title {
       background: url("../images/top_bg.png") no-repeat;
    }
}


.index-header-title .header-title-text{
    width: auto;
    height: 35px;
    line-height: 35px;
    float: right;
    margin-right: 20px;
    margin-top: -35px;
    color: #ffffff;
}
.index-header-title .title-href{
    display: inline-block;
    color: #ffffff;
}

.index-header-phone{
    float: right;
    margin-top: -34px;
    font-size: 22px;
}

.index-header-content{
    margin-top: 24px;
}
.index-header-content .index-header-search{
    padding-top: 24px;
}
.index-header-nav{
    margin-top: 48px;
}
.index-header-nav .nav-style{
    width: 122px;
}
.index-header-carousel{
    margin-bottom: 10px;
}
.nav>li>a{
    text-align: center;
 }

.dropdown-menu>li>a{
    text-align: center;
}
/*主体内容*/
.index-body{
    margin: 5px auto;
}

.index-body-carousel{
    width: 324px;
    height: 200px;
    float: left;
    border: solid 1px #2e415a;
}

img{
    width:100%;
    height: 100%;
}
.index-body .public-container{
    width: 990px;
}

.index-body .notice{
    margin-left: 8px;
    width: 324px;
    height: 200px;
    float: left;
    border: solid 1px #2e415a;
}
.index-body .notice .title{
    height: 35px;
    line-height: 35px;
    background-color: #dedede;
}


.index-body .notice .title a{
    color: #535353;
    font-size: 15px;
    background-color: #ffffff;
    display: inline-block;
    font-weight: normal;
    padding: 0 20px;
    border-right: solid 1px #2e415a;
    border-bottom: solid 1px #ffffff;
    text-decoration: none;
}

.index-body .notice .title .more{
    position: relative;
    float: right;
    margin-top: 6px;
    margin-right: 6px;
    border-right: none;
    width: 50px;
    height: 30px;
    display: inline-block;
    background: url('../images/more.png') no-repeat 0 0;

}

.notice .notice-list{
    padding: 6px 12px;
}
.notice .notice-list .new{
    background: url('../images/new.gif') no-repeat;
    display: inline-block;
    width: 25px;
    height: 11px;
    margin-right: 2px;
}
.notice .notice-list .notice-list-item{
    width: 220px;
    font-weight: normal;
    color: #000;
    text-decoration: none;
    display: inline-block;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.notice .notice-list span{
    float: right;
    color: #999;
    display: inline-block;
}


.body-ad{
    display: inline-block;
    margin-top: 5px;
}

.supply-require{
    float: left;
    width: 692px;
    font-size: 14px;
}
.supply-require .title{
    height: 35px;
    line-height: 35px;
    border-bottom: solid 2px #2e415a;
    background-color: #dedede;
}
.supply-require .title .title-content{
    font-size: 15px;
    color: #ffffff;
    float: left;
    padding: 0 12px;
    background-color: #244496;
}

.supply-require .list{
    display: block;
    padding-left: 0;
}

.supply-require .list li{
    padding-left: 9px;
    padding-top: 9px;
    line-height: 30px;
    border: solid 1px #2e415a;
