package com.jyu.sati.entity;

/**
 * 
 * 平台管理员或超级管理员
 * 
 * @author 淋雨又调皮
 *
 */
public class Adminstrator {

	public final static int ADMIN_TYPE_SUPER = 1;// 超级管理员
	public final static int ADMIN_TYPE_PLATFORM = 2;// 平台管理员

	private Integer administratorId;// 管理员id

	private Integer pbiId;// 个人基本信息id

	private Integer adminType;// 管理员类型

	public Integer getAdministratorId() {
		return administratorId;
	}

	public void setAdministratorId(Integer administratorId) {
		this.administratorId = administratorId;
	}

	public Integer getPbiId() {
		return pbiId;
	}

	public void setPbiId(Integer pbiId) {
		this.pbiId = pbiId;
	}

	public Integer getAdminType() {
		return adminType;
	}

	public void setAdminType(Integer adminType) {
		this.adminType = adminType;
	}

	@Override
	public String toString() {
		return "Adminstrator [administratorId=" + administratorId + ", pbiId=" + pbiId + ", adminType=" + adminType
				+ "]";
	}

}