}

.member-box .title .title-content{
    font-size: 15px;
    font-weight: normal;
    display: inline-block;
    padding: 4px 12px;
    margin-bottom: -3px;
    cursor: pointer;
    color: #000000;
    border-bottom: solid 3px #ff0000;
}
.member-box .list{
    display: block;
    padding-left: 0;
    margin-bottom: 0;
}
.member-box .list :last-child{
    border-bottom :none;
}
.member-box .list li{
    height: 43px;
    line-height: 43px;
    font-size: 13px;
    padding:0 6px;
    border-bottom: dotted 1px #2e415a;
}
.member-box .list li .list-left-hot{
    display: inline-block;
    height: 8px;
    width: 8px;
    margin-bottom: 18px;
    margin-right: 4px;
    background: url("../images/icons.png") no-repeat 2px -18px;
}
.member-box .list li a{
    width: 188px;
    font-weight: normal;
    color: #000000;
    text-decoration: none;
    display: inline-block;
    white-space: nowrap;
    word-break: keep-all;
    overflow: hidden;
    text-overflow: ellipsis;
}

.member-box .list li span{
    float: right;
    padding-right: 4px;
    color: #999999;
    display: inline-block;
}


.column{
    position: relative;
    width: 100%;
    float: left;
}
.column .title{
    height: 35px;
    line-height: 35px;
}
.column .title .title-icon{
    position: absolute;
    width: 20px;
    height: 20px;
    top:7px;
    margin-left: 4px;
    display: inline-block;
    background: url("../images/icons.png") no-repeat 0 -95px;
}
.column .title .title-content{
    font-size: 15px;
    display: inline-block;
    color: #f00;
    padding-left: 16px;
    cursor: pointer;
}
.column .title .more{
    position: absolute;
    top: 14px;
    right: 1px;
    width: 50px;
    height: 30px;
    margin-top: -6px;
    display: inline-block;
    background: url("../images/more.png") no-repeat 0 0;
}

.column .product-list{
    border: solid 1px #2e415a;
    padding: 8px;
    margin-top: 10px;
    overflow: hidden;
}
.column .product-list.marquee-list{
    list-style-type: none;
}
.column .product-list .marquee-item{
    float: left;
    display: block;
    width: 168px !important;
    border-radius: 0.7em;
    background: #eeeeee;
    margin: 0 10px;
    padding: 5px 5px 0 5px;
}

.column .product-list .marquee-item .marquee-image{
    cursor: auto;
    width: 100%;
    margin-top: 0.75em;
    display: block;
    height: 100px;
}

.column .product-list .marquee-item .marquee-content {
    display:block;
    width:154px;
    padding:2px 0;
    text-align:center;
    white-space: nowrap;
    word-break: keep-all;
    overflow: hidden;
    text-overflow: ellipsis;
}

/*尾部*/
